{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "react/prop-types": 0,
    "strict": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-use-before-define": ["error", { "functions": false }]
  }
}